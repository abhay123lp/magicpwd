                <action id="change-link" class="com.magicpwd.e.mpwd.edit.ChangeLinkAction" />
            </item>
            <!--转换为邮件-->
            <item type="normal" text="$P30F7D15">
                <action id="change-mail" class="com.magicpwd.e.mpwd.edit.ChangeMailAction" />
            </item>
            <!--转换为日期-->
            <item type="normal" text="$P30F7D16">
                <action id="change-date" class="com.magicpwd.e.mpwd.edit.ChangeDateAction" />
