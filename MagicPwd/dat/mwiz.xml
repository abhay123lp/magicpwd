<?xml version="1.0" encoding="UTF-8"?>
<magicpwd>
    <!--魔方密码工具栏-->
    <toolbar id="mwiz">
        <!--查看-->
        <item type="normal" text="$P30F6701" tips="$P30F6702">
            <icon>
                <default id="view-keys" path="var:mwiz-view"/>
            </icon>
            <action id="view-keys" class="com.magicpwd.e.mwiz.ViewAction">
                <stroke key="^ V" />
            </action>
        </item>
        <seperator />
        <!--新增-->
        <item type="normal" text="$P30F6703" tips="$P30F6704">
            <icon>
                <default id="new-keys" path="var:mwiz-add"/>
            </icon>
            <action id="new-keys" class="com.magicpwd.e.mwiz.NewAction">
                <stroke key="^ N" />
            </action>
        </item>
        <!--编辑-->
        <item type="normal" text="$P30F6705" tips="$P30F6706">
            <icon>
                <default id="edit-keys" path="var:mwiz-edit"/>
            </icon>
            <action id="edit-keys" class="com.magicpwd.e.mwiz.EditAction">
                <stroke key="^ E" />
            </action>
        </item>
        <!--删除-->
        <item type="normal" text="$P30F6707" tips="$P30F6708">
            <icon>
                <default id="delete-keys" path="var:mwiz-delete"/>
            </icon>
            <action id="delete-keys" class="com.magicpwd.e.mwiz.DeleteAction">
                <stroke key="^ D" />
            </action>
        </item>
        <seperator />
        <!--提醒-->
        <item id="hint" type="toggle" text="$P30F6709" tips="$P30F670A">
            <icon>
                <default id="hint-keys" path="var:mwiz-hint"/>
            </icon>
            <action id="hint-keys" class="com.magicpwd.e.mwiz.HintAction">
                <stroke key="^ R" />
            </action>
        </item>
        <!--查询-->
        <item id="find" type="toggle" text="$P30F670B" tips="$P30F670C">
            <icon>
                <default id="search-keys" path="var:mwiz-find"/>
            </icon>
            <action id="search-keys" class="com.magicpwd.e.mwiz.FindAction">
                <stroke key="^ F" />
            </action>
        </item>
    </toolbar>
    <popmenu id="mwiz">
        <!--查看记录-->
        <item type="normal" text="$P30F6D01">
            <action id="view-keys" class="com.magicpwd.e.mwiz.ViewAction" />
        </item>
        <seperator />
        <!--添加记录-->
        <item type="normal" text="$P30F6D02">
            <action id="new-keys" class="com.magicpwd.e.mwiz.NewAction" />
        </item>
        <!--更新记录-->
        <item type="normal" text="$P30F6D03">
            <action id="edit-keys" class="com.magicpwd.e.mwiz.EditAction" />
        </item>
        <!--删除记录-->
        <item type="normal" text="$P30F6D04">
            <action id="delete-keys" class="com.magicpwd.e.mwiz.DeleteAction" />
        </item>
        <seperator />
        <!--标记为：-->
        <menu text="$P30F7D1B">
            <!--默认(@0)-->
            <item type="checkbox" text="$P30F7D1C" group="label" command="0">
                <icon>
                    <default cache-id="label0" path="var:keys-label0"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <seperator />
            <!--常使用(@1)-->
            <item type="checkbox" text="$P30F7D1D" group="label" command="1">
                <icon>
                    <default cache-id="label1" path="var:keys-label1"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待注册(@2)-->
            <item type="checkbox" text="$P30F7D1E" group="label" command="2">
                <icon>
                    <default cache-id="label2" path="var:keys-label2"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待认证(@3)-->
            <item type="checkbox" text="$P30F7D1F" group="label" command="3">
                <icon>
                    <default cache-id="label3" path="var:keys-label3"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待激活(@4)-->
            <item type="checkbox" text="$P30F7D20" group="label" command="4">
                <icon>
                    <default cache-id="label4" path="var:keys-label4"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--仅测试(@5)-->
            <item type="checkbox" text="$P30F7D21" group="label" command="5">
                <icon>
                    <default cache-id="label5" path="var:keys-label5"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已过期(@6)-->
            <item type="checkbox" text="$P30F7D22" group="label" command="6">
                <icon>
                    <default cache-id="label6" path="var:keys-label6"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已丢失(@7)-->
            <item type="checkbox" text="$P30F7D23" group="label" command="7">
                <icon>
                    <default cache-id="label7" path="var:keys-label7"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--被禁用(@8)-->
            <item type="checkbox" text="$P30F7D24" group="label" command="8">
                <icon>
                    <default cache-id="label8" path="var:keys-label8"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已删除(@9)-->
            <item type="checkbox" text="$P30F7D25" group="label" command="9">
                <icon>
                    <default cache-id="label9" path="var:keys-label9"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
        </menu>
        <!--重要性：-->
        <menu text="$P30F7D26">
            <!--最高-->
            <item type="checkbox" text="$P30F7D27" group="major" command="2">
                <icon>
                    <default cache-id="major+2" path="var:keys-major+2"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--高-->
            <item type="checkbox" text="$P30F7D28" group="major" command="1">
                <icon>
                    <default cache-id="major+1" path="var:keys-major+1"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--正常-->
            <item type="checkbox" text="$P30F7D29" group="major" command="0">
                <icon>
                    <default cache-id="major0" path="var:keys-major0"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--低-->
            <item type="checkbox" text="$P30F7D2A" group="major" command="-1">
                <icon>
                    <default cache-id="major-1" path="var:keys-major-1"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--最低-->
            <item type="checkbox" text="$P30F7D2B" group="major" command="-2">
                <icon>
                    <default cache-id="major-2" path="var:keys-major-2"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
        </menu>
        <!--排序方式-->
        <menu text="$P30F7D2C">
            <!--升序-->
            <item type="checkbox" text="$P30F7D2D" group="order-dir" command="true">
                <action id="sort-dir" class="com.magicpwd.e.mwiz.SortDirAction" />
            </item>
            <!--降序-->
            <item type="checkbox" text="$P30F7D2E" group="order-dir" command="false">
                <action id="sort-dir" class="com.magicpwd.e.mwiz.SortDirAction" />
            </item>
        </menu>
        <!--排序依据-->
        <menu text="$P30F7D2F">
            <!--使用频率-->
            <item type="checkbox" text="$P30F7D30" group="order-key" command="01">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--口令名称-->
            <item type="checkbox" text="$P30F7D31" group="order-key" command="09">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--注册时间-->
            <item type="checkbox" text="$P30F7D32" group="order-key" command="07">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--到期时间-->
            <item type="checkbox" text="$P30F7D33" group="order-key" command="0D">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
        </menu>
    </popmenu>
    <!--日期弹出菜单选项-->
    <submenu id="date-template">
        <!--日期及时间-->
        <menu text="日期时间型">
            <item type="checkbox" text="完整格式" group="date-template" command="java:0d0t" />
            <item type="checkbox" text="长格式" group="date-template" command="java:1d1t" />
            <item type="checkbox" text="标准格式" group="date-template" command="java:2d2t" />
            <item type="checkbox" text="短格式" group="date-template" command="java:3d3t" />
            <seperator />
            <item type="checkbox" text="yyyyMMdd HHmmss 格式" group="date-template" command="user:yyyyMMdd HHmmss" />
            <item type="checkbox" text="yyyy-MM-dd HH:mm:ss 格式" group="date-template" command="user:yyyy-MM-dd HH:mm:ss" />
            <item type="checkbox" text="yyyy.MM.dd HH.mm.ss 格式" group="date-template" command="user:yyyy.MM.dd HH.mm.ss" />
            <item type="checkbox" text="yyyy年MM月dd日 HH时mm分ss秒 格式" group="date-template" command="user:yyyy年MM月dd日 HH时mm分ss秒" />
        </menu>
        <!--日期-->
        <menu text="日期型">
            <!--系统定义-->
            <item type="checkbox" text="完整格式" group="date-template" command="java:0d" />
            <item type="checkbox" text="长格式" group="date-template" command="java:1d" />
            <item type="checkbox" text="标准格式" group="date-template" command="java:2d" />
            <item type="checkbox" text="短格式" group="date-template" command="java:3d" />
            <seperator />
            <!--用户定义-->
            <item type="checkbox" text="yyyyMMdd 格式" group="date-template" command="user:yyyyMMdd" />
            <item type="checkbox" text="yyyy-MM-dd 格式" group="date-template" command="user:yyyy-MM-dd" />
            <item type="checkbox" text="yyyy/MM/dd 格式" group="date-template" command="user:yyyy/MM/dd" />
            <item type="checkbox" text="yyyy.MM.dd 格式" group="date-template" command="user:yyyy.MM.dd" />
            <item type="checkbox" text="yyyy MM dd 格式" group="date-template" command="user:yyyy MM dd" />
            <item type="checkbox" text="yyyy年MM月dd日 格式" group="date-template" command="user:yyyy年MM月dd日" />
        </menu>
        <!--时间-->
        <menu text="时间型">
            <item type="checkbox" text="完整格式" group="date-template" command="java:0t" />
            <item type="checkbox" text="长格式" group="date-template" command="java:1t" />
            <item type="checkbox" text="标准格式" group="date-template" command="java:2t" />
            <item type="checkbox" text="短格式" group="date-template" command="java:3t" />
            <seperator />
            <item type="checkbox" text="HHmmss 格式" group="date-template" command="user:HHmmss" />
            <item type="checkbox" text="HH:mm:ss 格式" group="date-template" command="user:HH:mm:ss" />
            <item type="checkbox" text="HH.mm.ss 格式" group="date-template" command="user:HH.mm.ss" />
            <item type="checkbox" text="HH mm ss 格式" group="date-template" command="user:HH mm ss" />
            <item type="checkbox" text="HH时mm分ss秒 格式" group="date-template" command="user:HH时mm分ss秒" />
        </menu>
    </submenu>
    <submenu id="date-interval">
        <item type="normal" text="09:30" command="now:date 09:30:00" />
        <item type="normal" text="18:00" command="now:date 18:00:00" />
        <seperator />
        <!--
        <item type="normal" text="2010-12-31 20:00:00" command="fix:2010-12-31 20:00:00" />
        <seperator />
        -->
        <menu text="秒">
            <item type="normal" text="30秒后" command="var:+30second" />
        </menu>
        <menu text="分">
            <item type="normal" text="30分后" command="var:+30minute" />
        </menu>
        <menu text="时">
            <item type="normal" text="1小时后" command="var:+1hour" />
            <item type="normal" text="2小时后" command="var:+2hour" />
        </menu>
        <menu text="天">
            <item type="normal" text="1天后" command="var:+1day" />
            <item type="normal" text="2天后" command="var:+2day" />
        </menu>
        <menu text="周">
            <item type="normal" text="1周后" command="var:+1week" />
            <item type="normal" text="2周后" command="var:+2week" />
        </menu>
        <menu text="月">
            <item type="normal" text="1月后" command="var:+1month" />
            <item type="normal" text="2月后" command="var:+2month" />
        </menu>
        <menu text="年">
            <item type="normal" text="1年后" command="var:+1year" />
            <item type="normal" text="2年后" command="var:+2year" />
        </menu>
    </submenu>
    <!--数值弹出菜单选项-->
    <submenu id="data-options">
        <!--整数-->
        <item type="checkbox" text="$P30F7C09" group="data-template" command="dec:0" />
        <!--小数-->
        <menu text="$P30F7C0A">
            <item type="checkbox" text="1位" group="data-template" command="dec:1" />
            <item type="checkbox" text="2位" group="data-template" command="dec:2" />
            <item type="checkbox" text="3位" group="data-template" command="dec:3" />
            <item type="checkbox" text="4位" group="data-template" command="dec:4" />
            <item type="checkbox" text="5位" group="data-template" command="dec:5" />
            <item type="checkbox" text="6位" group="data-template" command="dec:6" />
            <seperator />
            <!--其它……-->
            <item type="checkbox" id="data-precision" text="$P30F7C0B" group="data-template" command="dec:-1" />
            <seperator />
            <item type="checkbox" id="data-fixed-dec" text="$P30F7C17" command="dec:f" />
            <!--
            <item type="checkbox" id="data-fixed-int" text="$P30F7C18" command="int:f" />
            -->
        </menu>
        <!--集合-->
        <menu text="$P30F7C0C">
            <!--正数-->
            <item type="checkbox" id="data-pos" text="$P30F7C0D" command="set:+" />
            <!--0-->
            <item type="checkbox" id="data-0" text="$P30F7C0E" command="set:0" />
            <!--负数-->
            <item type="checkbox" id="data-nav" text="$P30F7C0F" command="set:-" />
        </menu>
        <!--符号-->
        <menu text="$P30F7C10">
            <!--启用特殊符号-->
            <item type="checkbox" id="data-notation" text="$P30F7C11" command="notation"/>
            <seperator />
            <!--更改特殊符号-->
            <item type="normal" id="data-character" tips="$P30F7C13" command="character"/>
            <!--符号前置-->
            <item type="checkbox" id="data-thehead" text="$P30F7C14" group="data-position" command="pos:^" />
            <!--符号后置-->
            <item type="checkbox" id="data-thefoot" text="$P30F7C15" group="data-position" command="pos:$" />
            <!--可选输入-->
            <item type="checkbox" id="data-optional" text="$P30F7C16" command="optional"/>
        </menu>
    </submenu>
    <strokes id="mwiz">
        <action id="exit-window" class="com.magicpwd.e.tray.ExitAction">
            <stroke key="^ Q" />
        </action>
        <action id="lock-window" class="com.magicpwd.e.mwiz.LockAction">
            <stroke key="^ L" />
        </action>
        <action id="hide-window" class="com.magicpwd.e.mwiz.HideAction">
            <stroke key="^ H" />
            <stroke key="^ Enter" />
        </action>
        <action id="show-mpwd" class="com.magicpwd.e.tray.MpwdAction">
            <stroke key="^ # P" />
        </action>
        <action id="show-mpad" class="com.magicpwd.e.tray.MpadAction">
            <stroke key="^ # N" />
        </action>
    </strokes>
</magicpwd>