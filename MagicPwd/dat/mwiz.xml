<?xml version="1.0" encoding="UTF-8"?>
<magicpwd>
    <!--魔方密码工具栏-->
    <toolbar id="mwiz">
        <!--查看-->
        <item type="normal" text="$P30F6701" tips="$P30F6702">
            <icon>
                <default id="view-keys" path="skin/feel/%feel%/mwiz-view.png"/>
            </icon>
            <action id="view-keys" class="com.magicpwd.e.mwiz.ViewAction">
                <stroke key="^ V" />
            </action>
        </item>
        <seperator />
        <!--新增-->
        <item type="normal" text="$P30F6703" tips="$P30F6704">
            <icon>
                <default id="new-keys" path="skin/feel/%feel%/mwiz-add.png"/>
            </icon>
            <action id="new-keys" class="com.magicpwd.e.mwiz.NewAction">
                <stroke key="^ N" />
            </action>
        </item>
        <!--编辑-->
        <item type="normal" text="$P30F6705" tips="$P30F6706">
            <icon>
                <default id="edit-keys" path="skin/feel/%feel%/mwiz-edit.png"/>
            </icon>
            <action id="edit-keys" class="com.magicpwd.e.mwiz.EditAction">
                <stroke key="^ E" />
            </action>
        </item>
        <!--删除-->
        <item type="normal" text="$P30F6707" tips="$P30F6708">
            <icon>
                <default id="delete-keys" path="skin/feel/%feel%/mwiz-delete.png"/>
            </icon>
            <action id="delete-keys" class="com.magicpwd.e.mwiz.DeleteAction">
                <stroke key="^ D" />
            </action>
        </item>
        <seperator />
        <!--查询-->
        <item id="search" type="toggle" text="$P30F6709" tips="$P30F670A">
            <icon>
                <default id="search-keys" path="skin/feel/%feel%/mwiz-search.png"/>
            </icon>
            <action id="search-keys" class="com.magicpwd.e.mwiz.SearchAction">
                <stroke key="^ F" />
            </action>
        </item>
    </toolbar>
    <popmenu id="mwiz">
        <!--查看记录-->
        <item type="normal" text="$P30F7D05">
            <action id="view-keys" class="com.magicpwd.e.mwiz.ViewAction" />
        </item>
        <seperator />
        <!--添加记录-->
        <item type="normal" text="$P30F7D05">
            <action id="new-keys" class="com.magicpwd.e.mwiz.NewAction" />
        </item>
        <!--更新记录-->
        <item type="normal" text="$P30F7D05">
            <action id="edit-keys" class="com.magicpwd.e.mwiz.EditAction" />
        </item>
        <!--删除记录-->
        <item type="normal" text="$P30F7D06">
            <action id="delete-keys" class="com.magicpwd.e.mwiz.DeleteAction" />
        </item>
        <seperator />
        <!--标记为：-->
        <menu text="$P30F7D1B">
            <!--默认(@0)-->
            <item type="checkbox" text="$P30F7D1C" group="label" command="0">
                <icon>
                    <default cache-id="label0" path="skin/feel/%feel%/label.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <seperator />
            <!--常使用(@1)-->
            <item type="checkbox" text="$P30F7D1D" group="label" command="1">
                <icon>
                    <default cache-id="label1" path="skin/feel/%feel%/label1.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待注册(@2)-->
            <item type="checkbox" text="$P30F7D1E" group="label" command="2">
                <icon>
                    <default cache-id="label2" path="skin/feel/%feel%/label2.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待认证(@3)-->
            <item type="checkbox" text="$P30F7D1F" group="label" command="3">
                <icon>
                    <default cache-id="label3" path="skin/feel/%feel%/label3.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--待激活(@4)-->
            <item type="checkbox" text="$P30F7D20" group="label" command="4">
                <icon>
                    <default cache-id="label4" path="skin/feel/%feel%/label4.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--仅测试(@5)-->
            <item type="checkbox" text="$P30F7D21" group="label" command="5">
                <icon>
                    <default cache-id="label5" path="skin/feel/%feel%/label5.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已过期(@6)-->
            <item type="checkbox" text="$P30F7D22" group="label" command="6">
                <icon>
                    <default cache-id="label6" path="skin/feel/%feel%/label6.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已丢失(@7)-->
            <item type="checkbox" text="$P30F7D23" group="label" command="7">
                <icon>
                    <default cache-id="label7" path="skin/feel/%feel%/label7.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--被禁用(@8)-->
            <item type="checkbox" text="$P30F7D24" group="label" command="8">
                <icon>
                    <default cache-id="label8" path="skin/feel/%feel%/label8.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
            <!--已删除(@9)-->
            <item type="checkbox" text="$P30F7D25" group="label" command="9">
                <icon>
                    <default cache-id="label9" path="skin/feel/%feel%/label9.png"/>
                </icon>
                <action id="change-mode" class="com.magicpwd.e.mwiz.ChangeLabelAction" />
            </item>
        </menu>
        <!--重要性：-->
        <menu text="$P30F7D26">
            <!--最高-->
            <item type="checkbox" text="$P30F7D27" group="major" command="2">
                <icon>
                    <default cache-id="major2" path="skin/feel/%feel%/major+2.png"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--高-->
            <item type="checkbox" text="$P30F7D28" group="major" command="1">
                <icon>
                    <default cache-id="major1" path="skin/feel/%feel%/major+1.png"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--正常-->
            <item type="checkbox" text="$P30F7D29" group="major" command="0">
                <icon>
                    <default cache-id="major0" path="skin/feel/%feel%/major.png"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--低-->
            <item type="checkbox" text="$P30F7D2A" group="major" command="-1">
                <icon>
                    <default cache-id="major-1" path="skin/feel/%feel%/major-1.png"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
            <!--最低-->
            <item type="checkbox" text="$P30F7D2B" group="major" command="-2">
                <icon>
                    <default cache-id="major-2" path="skin/feel/%feel%/major-2.png"/>
                </icon>
                <action id="change-note" class="com.magicpwd.e.mwiz.ChangeMajorAction" />
            </item>
        </menu>
        <!--排序方式-->
        <menu text="$P30F7D2C">
            <!--升序-->
            <item type="checkbox" text="$P30F7D2D" group="order-dir" command="true">
                <action id="sort-dir" class="com.magicpwd.e.mwiz.SortDirAction" />
            </item>
            <!--降序-->
            <item type="checkbox" text="$P30F7D2E" group="order-dir" command="false">
                <action id="sort-dir" class="com.magicpwd.e.mwiz.SortDirAction" />
            </item>
        </menu>
        <!--排序依据-->
        <menu text="$P30F7D2F">
            <!--使用频率-->
            <item type="checkbox" text="$P30F7D30" group="order-key" command="01">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--口令名称-->
            <item type="checkbox" text="$P30F7D31" group="order-key" command="09">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--注册时间-->
            <item type="checkbox" text="$P30F7D32" group="order-key" command="07">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
            <!--到期时间-->
            <item type="checkbox" text="$P30F7D33" group="order-key" command="0D">
                <action id="sort-key" class="com.magicpwd.e.mwiz.SortKeyAction" />
            </item>
        </menu>
    </popmenu>
    <submenu id="date-interval">
        <menu text="秒">
            <item type="normal" text="30秒后" command="time:30second" />
        </menu>
        <menu text="分">
            <item type="normal" text="30分后" command="time:30minute" />
        </menu>
        <menu text="时">
            <item type="normal" text="1小时后" command="time:1hour" />
            <item type="normal" text="2小时后" command="time:2hour" />
        </menu>
        <menu text="天">
            <item type="normal" text="1天后" command="time:1day" />
            <item type="normal" text="2天后" command="time:2day" />
        </menu>
        <menu text="周">
            <item type="normal" text="1周后" command="time:1week" />
        </menu>
        <menu text="月">
            <item type="normal" text="1月后" command="time:1month" />
        </menu>
        <menu text="年">
            <item type="normal" text="1年后" command="time:1year" />
        </menu>
    </submenu>
    <strokes id="mwiz">
        <action id="lock-window" class="com.magicpwd.e.mwiz.LockAction">
            <stroke key="^ L" />
        </action>
        <action id="hide-window" class="com.magicpwd.e.mwiz.HideAction">
            <stroke key="^ H" />
            <stroke key="^ Enter" />
        </action>
        <action id="show-mpwd" class="com.magicpwd.e.tray.MpwdAction">
            <stroke key="^ # P" />
        </action>
        <action id="show-mpad" class="com.magicpwd.e.tray.MpadAction">
            <stroke key="^ # N" />
        </action>
    </strokes>
</magicpwd>