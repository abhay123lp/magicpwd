<?xml version="1.0" encoding="UTF-8"?>
<magicpwd>
    <base>
        <name>点阵格式</name>
        <type>png</type>
    </base>
    <card>
        <background color="#ffff00" image="splash.png" type="scale"></background>
        <draw method="text" x="10" y="20" color="FF0000" font-name="行楷" font-size="16" font-style="3">#姓##名#</draw>
        <draw method="image" x="30" y="30" width="" height="">http://magicpwd.com/logo/logo20.png</draw>
        <draw method="line" x1="40" y1="40" x2="160" y2="40" color="#0000ff"></draw>
        <draw method="rect" x="1" y="1" width="200" height="100" border-width="5" border-color="abcdef" />
        <draw method="round-rect" x="1" y="1" width="150" height="50" arc-width="10" arc-height="10" color="#ffffff" border-width="2" border-color="#ff0000" />
        <draw method="arc" x="50" y="50" width="100" height="60" start-angle="20" arc-angle="30" color="#555555" border-width="3" border-color="#FFF000" />
        <draw method="polygon" x-array="10,20,30,40" y-array="10,20,20,10" color="#0000FF" border-width="" border-color="00FFFF" />
    </card>
</magicpwd>