<?xml version="1.0" encoding="UTF-8"?>
<magicpwd>
    <base>
        <name>测试卡片</name>
        <type>htm/txt/png/svg/*</type>
    </base>
    <!--txt-->
    <card>
        <template-uri></template-uri>
    </card>
    <!--htm-->
    <card>
        <template-uri></template-uri>
        <template-res></template-res>
    </card>
    <!--png-->
    <card>
        <size width="" height=""></size>
        <background></background>
        <draw method="text" x="" y="" color="" font-name="" font-size=""></draw>
        <draw method="image" x="" y="" width="" height=""></draw>
        <draw method="rect" x="" y="" width="" height=""></draw>
        <draw method="3drect" x="" y="" width="" height=""></draw>
        <draw method="round-rect" x="" y="" width="" height=""></draw>
        <draw method="arc" x="" y="" width="" height=""></draw>
        <draw method="polygon" x="" y="" width="" height=""></draw>
        <draw method="line" x="" y="" width="" height=""></draw>
    </card>
    <!--svg-->
    <card>
        <template-uri></template-uri>
    </card>
</magicpwd>