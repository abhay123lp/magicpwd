<?xml version="1.0" encoding="UTF-8"?>
<magicpwd>
    <strokes id="mpad">
        <action id="file-search" class="com.magicpwd.e.mpad.FindAction">
            <stroke key="^ F" />
        </action>
        <action id="lock-window" class="com.magicpwd.e.mpad.LockAction">
            <stroke key="^ L" />
        </action>
        <action id="hide-window" class="com.magicpwd.e.mpad.HideAction">
            <stroke key="^ H" />
            <stroke key="^ Enter" />
        </action>
    </strokes>
</magicpwd>